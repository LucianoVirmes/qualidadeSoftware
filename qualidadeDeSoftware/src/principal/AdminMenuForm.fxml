<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Menu?>
<?import javafx.scene.control.MenuBar?>
<?import javafx.scene.control.MenuItem?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.Pane?>

<BorderPane fx:id="bpPrincipalAdmin" style="-fx-background-color: #668B8B;" xmlns="http://javafx.com/javafx/8.0.141" xmlns:fx="http://javafx.com/fxml/1" fx:controller="principal.AdminMenuController">
   <center>
      <Pane prefHeight="600.0" prefWidth="800.0" BorderPane.alignment="CENTER" />
   </center>
   <top>
      <MenuBar style="-fx-background-color: #FFFFFF;" BorderPane.alignment="CENTER">
        <menus>
          <Menu mnemonicParsing="false" text="Admin">
            <items>
                  <MenuItem fx:id="mmiEditarAdmin" mnemonicParsing="false" onAction="#menuEditarAdmin" text="Editar Admin" />
                  <MenuItem fx:id="mmiAdicionarAdmin" mnemonicParsing="false" onAction="#menuAddAdmin" text="Adcionar Admin" />
            </items>
          </Menu>
          <Menu mnemonicParsing="false" text="Produtos">
               <items>
                  <MenuItem fx:id="mmiProdutos" mnemonicParsing="false" onAction="#menuAddProdutos" text="produtos" />
               </items>
          </Menu>
          <Menu mnemonicParsing="false" text="Categoria">
            <items>
              <MenuItem fx:id="mmiCategorias" mnemonicParsing="false" onAction="#menuCategorias" text="Visualizar categorias" />
            </items>
          </Menu>
        </menus>
      </MenuBar>
   </top>
</BorderPane>
